{
  "name": "crm-consulting-backend",
  "version": "1.0.0",
  "description": "CRM system backend with social media customer discovery",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "test": "node scripts/test-runner.js --all",
    "test:watch": "jest --watch",
    "test:pipeline": "node scripts/test-pipeline-postgres.js",
    "test:pipeline:runner": "node scripts/test-runner.js --pipeline",
    "test:setup": "node scripts/test-runner.js --setup-only",
    "test:controllers": "node scripts/test-runner.js --pattern=controllers",
    "test:routes": "node scripts/test-runner.js --pattern=routes",
    "test:models": "node scripts/test-runner.js --pattern=models",
    "test:integration": "node scripts/test-runner.js --pattern=integration",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "keywords": [
    "crm",
    "nodejs",
    "express",
    "postgresql",
    "sequelize",
    "jwt",
    "social-media"
  ],
  "author": "CRM Consulting Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.10",
    "sequelize": "^6.35.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
